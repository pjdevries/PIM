  "assets": [
    {
      "name": "obix.uploadCss",
      "type": "style",
      "uri": "obix/upload.css"
    },
    {
      "name": "obix.uploadJs",
      "type": "script",
      "uri": "obix/upload.js",
      "attributes": {
